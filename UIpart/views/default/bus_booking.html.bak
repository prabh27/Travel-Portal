{{extend 'layout.html'}}
<head>
<style>
    .PageForm tr td{
        font-weight:bold;
        font-size:16px;
        
        }
        
  
        </style>
</head>

<div><center>{{=form.custom.begin}}
    <table class="PageForm">
    <tr>
        <td>From</td> <td>To</td>
        </tr>
        <tr>
            <td>{{=form.custom.widget.From}}</td><td>{{=form.custom.widget.To}}</td>
        
        </tr>
        <tr>
        <td>Date
            </td>
            
            <td>Number of Seats
            </td>
        </tr>
        <tr>
            <td>{{=form.custom.widget.d}}
            </td>
            <td>{{=form.custom.widget.seats}}
            </td>
            
        </tr>
    </table>
    {{=form.custom.submit}}    
    
    {{=form.custom.end}}
    </center>
</div>
